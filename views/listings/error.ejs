<% layout("layouts/boilerplate.ejs") -%>

<section class="page page-error">
  <div class="error-card">
    <div class="error-icon">
      <i class="fa-solid fa-triangle-exclamation"></i>
    </div>
    <h1>Error <%= statusCode %></h1>
